{
    "resource": "/brands",
    "useCases": {
        "insert": "AddBrand",
        "search": "SearchBrands",
        "update": "UpdateBrand",
        "delete": "DeleteBrand"
      },
    "insertFields": [
        {
            "key":"name",
            "label":"$name",
